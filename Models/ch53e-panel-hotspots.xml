<?xml version="1.0"?>
<PropertyList>

 <name>Panel Hotspots</name>
 <background>Aircraft/b29/Models/transparent.rgb</background>
 <w>1024</w>
 <h>1024</h>
 <y-offset>0</y-offset>
  
 <instruments>
  <instrument>
   <name>Panel</name>
   <x>512</x>
   <y>512</y>
   <w>1024</w>
   <h>1024</h>
   <w-base>1024</w-base>
   <h-base>1024</h-base>
   <actions>
