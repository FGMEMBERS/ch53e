    <action>
     <name>testCautionLights</name>
     <button>0</button>
     <x>-138</x>
     <y>-47</y>
     <w>6</w>
     <h>24</h>
     <binding>
      <command>property-assign</command>
      <property>sim/model/ch53e/control-input/caution-test</property>
      <value>1</value>
     </binding>
     <mod-up>
      <binding>
       <command>property-assign</command>
       <property>sim/model/ch53e/control-input/caution-test</property>
       <value>0</value>
      </binding>
     </mod-up>
    </action>
   </actions>
  </instrument>
