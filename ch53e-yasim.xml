<airplane mass="33228">
	
	<!-- Approach configuration -->
	<approach speed="90" aoa="8">
		<control-setting axis="/controls/engines/engine[0]/throttle" value="0.1"/>
	</approach>
	
	<!-- Cruise configuration -->
	<cruise speed="220" alt="0">
		<control-setting axis="/controls/engines/engine[0]/throttle" value="1.0"/>
	</cruise>
	
	<!-- 3000000 in*lbs torque on d model according to kegwasher at t-online-->
	<rotor name="main" x="-9.126" y="0.0" z="3.686" nx="0.07" ny="0" nz="1.0" fx="1" fy="0" fz="0"
		diameter="22" rel_len_blade_start="0.29"
      chord=".78" twist="-0.8"
      numblades="7" weightperblade="800" relbladecenter="0.61"
      rel_len_where_incidence_is_measured="0.8"

		maxcollective="15.8" mincollective="0.2"
		mincyclicele="-7.6" maxcyclicele="7.6"
		mincyclicail="-8.94" maxcyclicail="8.94"

		flapmin="-18" flapmax="18" flap0="-5"
		delta="0.28" rellenflaphinge="0.26" delta3="0.15"

		incidence_stall_zero_speed="15"
		incidence_stall_half_sonic_speed="14"
		lift_factor_stall="0.28"
		drag_factor_stall="1.5"
		stall_change_over="2.0"
		translift_ve="20"
		translift_maxfactor="1.3"
		ground_effect_constatnt="1.25"

		pitch_a="12.0" forceatpitch_a="69000"
		pitch_b="12.0" poweratpitch_b="9800"  
		poweratpitch_0="53"

		dynamic="1" number_of_segments="10"
		rpm="185" ccw="1" notorque="0" >
	
		<control-input axis="/controls/flight/aileron-trim" control="CYCLICAIL" split="1"/>
		<control-input axis="/controls/flight/aileron" control="CYCLICAIL" 
						src0="-1.0" src1="1.0"
						dst0="-1.0" dst1="1.0"/>	
	
		<control-input axis="/controls/flight/elevator-trim" control="CYCLICELE" split="1"/> 
		<control-input axis="/controls/flight/elevator" control="CYCLICELE" 
						src0="-1.0" src1="1.0"
						dst0="-1.0" dst1="1.0"/>
	
		<control-input axis="/controls/engines/engine[1]/throttle" control="COLLECTIVE"
						src0="-0.0" src1="1.0"
						dst0="1.0" dst1="-1.0"/>
	</rotor>
	
	<rotor name="tail" x="-24.096" y="1.989" z="4.471" nx="0.0" ny="-1" nz="-.36" fx="1" fy="0" fz="0"
		diameter="4.87" rel_len_blade_start="0.1"
      chord="0.32" twist="0"
		numblades="4" weightperblade="50" relbladecenter="0.58"
      rel_len_where_incidence_is_measured="0.8"

		maxcollective="20" mincollective="-10"

		flapmin="-8" flapmax="8" flap0="0" flap0factor="0"
		delta="0.5" rellenflaphinge="0.0" delta3="1"

		incidence_stall_zero_speed="15"
		incidence_stall_half_sonic_speed="14"
		lift_factor_stall="0.28"
		drag_factor_stall="1.5"
		stall_change_over="2.0"
		translift_ve="20"
		translift_maxfactor="1.3"

		pitch_a="15.8" forceatpitch_a="7500"
		pitch_b="15.8" poweratpitch_b="1010"
		poweratpitch_0="5"

		dynamic="0.5" number_of_segments="10"
		rpm="1100" ccw="1" notorque="0" >
	
		<control-input axis="/controls/flight/rudder" control="COLLECTIVE" invert="1"
			src0="-1.0" src1="1.0"
			dst0="-1.0" dst1="1.0"/>
	</rotor>

	<rotorgear
		max_power_engine="70000"
		engine_prop_factor="0.03"
		yasimdragfactor="10"
		yasimliftfactor="140"
	>

	<control-input axis="/controls/engines/engine[0]/magnetos" control="ROTORGEARENGINEON"/>
	<control-input axis="/controls/rotor/brake" control="ROTORBRAKE"
		src0="0.0" src1="1.0"
		dst0="0.0" dst1="1.0"/>

	</rotorgear>
		  
	<!-- FIXME what side is this on? also add the hstab -->
	<vstab x="-21.5" y="0" z="1.9" taper="0.75" dihedral="110"
		length="4.0" chord="2.3" sweep="30">
		<stall aoa="27" width="6" peak="1.3"/>
	</vstab>
	
	<fuselage ax="-3"    ay="0"  az="0.9" bx="-18.5" by="0"  bz="1.6"
		width="4"   taper="0.5" midpoint="0.35"/> <!-- Fuse -->
	<fuselage ax="-18.5" ay="0"  az="1.6" bx="-23"   by="0"  bz="1.6"
		width="2"   taper="0.5" midpoint="0"/>    <!-- Boom -->
	<fuselage ax="-6.8"  ay="3"  az="0"   bx="-12.9" by="3"  bz="0"
		width="1"   taper="0.5" midpoint="0.25"/> <!-- Sponson -->
	<fuselage ax="-6.8"  ay="-2" az="0"   bx="-12.9" by="-2" bz="0"
		width="1"   taper="0.5" midpoint="0.25"/> <!-- Sponson -->
	<fuselage ax="-6.1"  ay="2"  az="0"   bx="-12.9" by="2"  bz="0"
		width="0.8" taper="0.5" midpoint="0.5"/>  <!-- Drop Tank -->
	<fuselage ax="-6.1"  ay="-3" az="0"   bx="-12.9" by="-3" bz="0"
		width="0.8" taper="0.5" midpoint="0.5"/>  <!-- Drop Tank -->
	
	<cockpit x="-0.65" y="-1.3" z="4.4"/>
	
	<tank x="-9.3" y="2.8"  z="-1.0" capacity="4368" jet="true"/>
	<tank x="-9.3" y="1.9"  z="-1.0" capacity="5900" jet="true"/>
	<tank x="-9.3" y="-1.9" z="-1.0" capacity="5900" jet="true"/> 
	<tank x="-9.3" y="-2.8" z="-1.0" capacity="4368" jet="true"/>

	<!-- Internal Cargo -->
	<weight x="-6.374"  y="0" z="-0.7" mass-prop="/sim/weight[0]/weight-lb" size="0"/>
	<weight x="-7.898"  y="0" z="-0.7" mass-prop="/sim/weight[1]/weight-lb" size="0"/>
	<weight x="-9.422"  y="0" z="-0.7" mass-prop="/sim/weight[2]/weight-lb" size="0"/>
	<weight x="-10.946" y="0" z="-0.7" mass-prop="/sim/weight[3]/weight-lb" size="0"/>
	<weight x="-12.470" y="0" z="-0.7" mass-prop="/sim/weight[4]/weight-lb" size="0"/>
	<weight x="-13.994" y="0" z="-0.7" mass-prop="/sim/weight[5]/weight-lb" size="0"/>

	<!-- External Cargo -->
	<weight x="-9.5" y="0" z="-0.9"  mass-prop="/sim/weight[6]/weight-lb" size="6"/>
	<weight x="-5.9" y="1.9" z="1.1" mass-prop="/sim/weight[7]/weight-lb" size="0.5"/>
	
	<gear x="-3.44" y="0" z="-1.35" compression="0.4"  spring="1.2" damp="1.0"> 
		<control-input axis="/controls/flight/rudder" control="STEER" square="true"
			src0="-1" dst0="-1.5"
			src1="1"  dst1="1.5" />
		<control-input axis="/controls/gear/castering" control="CASTERING"/>
		<control-input axis="/controls/gear/gear-down" control="EXTEND"/>
		<control-speed control="EXTEND" transition-time="6"/>
		<control-output control="EXTEND" prop="/gear/gear[0]/position-norm"/>
	</gear>
	
	<gear x="-11.74" y="2.25" z="-1.35" compression=".4"  spring="1.2" damp="1.0"> 
		<control-input axis="/controls/gear/brake-left" control="BRAKE"/>
		<control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
		<control-input axis="/controls/gear/gear-down" control="EXTEND"/>
		<control-speed control="EXTEND" transition-time="6"/>
		<control-output control="EXTEND" prop="/gear/gear[2]/position-norm"/>
	</gear>
	
	<gear x="-11.74" y="-2.25" z="-1.35" compression=".4" spring="1.2" damp="1.0"> 
		<control-input axis="/controls/gear/brake-right" control="BRAKE"/>
		<control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
		<control-input axis="/controls/gear/gear-down" control="EXTEND"/>
		<control-speed control="EXTEND" transition-time="6"/>
		<control-output control="EXTEND" prop="/gear/gear[1]/position-norm"/>
	</gear>
	
	<gear x="-23.5" y="0" z="0.65" compression=".4" spring="1.2" damp="1.0"> 
		<control-input axis="/controls/gear/gear-down" control="EXTEND"/>
		<control-speed control="EXTEND" transition-time="6"/>
		<control-output control="EXTEND" prop="/gear/gear[3]/position-norm"/>
	</gear>
	
</airplane>
